  <style type="text/css">
    /* Define the styles for the footer */
    .footer {
        width: 200px;
        background-color: transparent;
        position: fixed;
        right: 0;
        bottom: 0;
        
    }

    /* Define the styles for the button inside the footer */
    .footer .btn {
        color: rgb(255, 255, 255);
        width: 200px;
        height: 80px;
        font-size: 60px;
        cursor: pointer;
        background-color: gray;
        border: 1px solid #000;
        transition: all .3s linear;
        border-radius: 50% 20% / 10% 40%;
    }

    .footer .btn p{
        position: relative;
        z-index: 1;     
    }


    </style>
        
        
